[
  {
    "name": "askGoalName",
    "type": "text",
    "text": " $(askGoalName) ",
    "answers": [ {"name": "goalName", "next": "askGoalImage" } ]
  },
  {
    "name": "askGoalImage",
    "type": "choice",
    "text": "$(askGoalImage)",
    "answers": [
      { "name": "Capture", "value": "Capture", "text":"Capture", "next": "askGoalImageTake"},
      { "name": "Skip", "value": "Skip", "text":"Skip", "next": "askKnowGoalAmount"}
    ]
  },
  {
    "name": "askGoalImageTake",
    "type": "imageUpload",
    "text": "$(askGoalImageTake)",
    "answers": [ {"name": "goalImage", "next": "askKnowGoalAmount" } ]
  },
  {
    "name": "askKnowGoalAmount",
    "type": "choice",
    "text": "$(askKnowGoalAmount)",
    "answers": [
      { "name": "knowAmountY", "value": "yes", "text":"$(knowAmountY)", "next": "askGoalAmount"},
      { "name": "knowAmountN", "value": "no", "text":"$(knowAmountN)", "next": "askGoalWeeklySaveAmount"}
    ]
  },
  {
    "name": "askGoalAmount",
    "type": "currency",
    "text": "$(askGoalAmount)",
    "error": "validationFail",
    "answers": [ { "name": "goalAmount", "next": "askKnowGoalDate" } ]
  },
  {
    "name": "infoDontKnowAmount",
    "type": "info",
    "text": "$(infoDontKnowAmount)",
    "next": "askGoalWeeklySaveAmount"
  },
  {
    "name": "askKnowGoalDate",
    "type": "choice",
    "text": "$(askKnowGoalDate)",
    "answers": [
      { "name": "knowDate", "value": "yes", "text":"Yes", "next": "askGoalDate"},
      { "name": "knowDate", "value": "no", "text":"No", "next": "askGoalWeeklySaveAmount"}
    ]
  },
  {
    "name": "askGoalDate",
    "type": "date",
    "text": "$(askGoalDate)",
    "error": "validationFailDate",
    "answers": [ { "name": "goalDate", "next": "verifyGoalAmount" } ]
  },
  {
    "name": "askGoalWeeklySaveAmount",
    "type": "choice",
    "text": "$(askGoalWeeklySaveAmount)",
    "answers": [
      { "name": "weeklySaveAmount", "value":  "500", "text":"Yes", "next": "askGoalPriorSave"},
      { "name": "weeklySaveAmount", "value": "1000", "text":"Yes", "next": "askGoalPriorSave"},
      { "name": "weeklySaveAmount", "value": "1500", "text":"Yes", "next": "askGoalPriorSave"},
      { "name": "weeklySaveAmount", "value": "2000", "text":"Yes", "next": "askGoalPriorSave"},
      { "name": "otherSaveAmount", "value": "other", "text": "Other", "next": "askGoalSaveAmountOther"}
    ]
  },
  {
    "name": "askGoalSaveAmountOther",
    "type": "currency",
    "text": "$(askGoalSaveAmountOther)",
    "error": "validationFailSaveAmountOther",
    "answers": [ { "name": "weeklySaveAmount", "next": "askGoalPriorSave" } ]
  },
  {
    "name": "askGoalPriorSave",
    "type": "choice",
    "text": "$(askGoalPriorSave)",
    "answers": [
      { "name": "hasSaved", "value": "yes", "text":"Yes", "next": "askPriorSavedAmount"},
      { "name": "hasSaved", "value": "no", "text":"No", "next": "verifyGoalAmount"}
    ]
  },
  {
    "name": "askPriorSavedAmount",
    "type": "currency",
    "text": "$(askPriorSavedAmount)",
    "error": "validationFailPriorSavedAmount",
    "answers": [ { "name": "priorSaveAmount", "next": "infoGoalEnd" } ]
  },
  {
    "name": "verifyGoalAmount",
    "type": "choice",
    "text": "$(verifyGoalAmount)",
    "error": "validationFailPriorSavedAmount",
    "answers": [
      { "name": "goalCheck", "value": "reduceWeek", "text":"Reduce Time", "next": "askKnowGoalDate"},
      { "name": "goalCheck", "value": "reduceAmount", "text":"Reduce Amount", "next": "askPriorSavedAmount"},
      { "name": "goalCheck", "value": "ok", "text":"OK", "next": "infoGoalEnd"}
    ]
  },
  {
    "name": "validationFail",
    "type": "info",
    "text": "$(validationFail)",
    "next": "askGoalAmount"
  },
  {
    "name": "validationFailDate",
    "type": "info",
    "text": "$(validationFailDate)",
    "next": "askGoalDate"
  },
  {
  "name": "validationFailSaveAmountOther",
  "type": "info",
  "text": "$(validationFailSaveAmountOther)",
  "next": "askGoalSaveAmountOther"
  },
  {
  "name": "validationFailPriorSavedAmount",
  "type": "info",
  "text": "$(validationFailPriorSavedAmount)",
  "next": "askPriorSavedAmount"
},
  {
    "name": "infoGoalEnd",
    "type": "end",
    "text": "$(infoGoalEnd)"
  }
]
