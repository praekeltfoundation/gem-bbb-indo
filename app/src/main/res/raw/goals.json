[
  {
    "name": "askGoalName",
    "type": "text",
    "text": " $(askGoalName) ",
    "answers": [
      {"name": "goalName", "next": "askGoalImage", "text":"$(addOwnGoal)" } ]
  },
  {
    "name": "askGoalImage",
    "type": "choice",
    "text": "$(askGoalImage)",
    "answers": [
      { "name": "Capture", "value": " ", "text":"$(Capture)", "next": "askGoalImageTake"},
      { "name": "Skip", "value": "Skip", "text":"$(Skip)", "next": "askKnowGoalAmount"}
    ]
  },
  {
    "name": "askGoalImageTake",
    "type": "imageUpload",
    "text": "$(askGoalImageTake)",
    "answers": [ {"name": "goalImage", "next": "askKnowGoalAmount" } ]
  },
  {
    "name": "askKnowGoalAmount",
    "type": "choice",
    "text": "$(askKnowGoalAmount)",
    "answers": [
      { "name": "knowAmountY", "value": "yes", "text":"$(knowAmountY)", "next": "askGoalAmount"},
      { "name": "knowAmountN", "value": "no", "text":"$(knowAmountN)", "next": "askGoalWeeklySaveAmount"}
    ]
  },
  {
    "name": "askGoalAmount",
    "type": "currency",
    "text": "$(askGoalAmount)",
    "error": "validationFail",
    "answers": [ { "name": "goalAmount", "next": "askKnowGoalDate" } ]
  },
  {
    "name": "infoDontKnowAmount",
    "type": "info",
    "text": "$(infoDontKnowAmount)",
    "next": "askGoalWeeklySaveAmount"
  },
  {
    "name": "askKnowGoalDate",
    "type": "choice",
    "text": "$(askKnowGoalDate)",
    "answers": [
      { "name": "knowDateY", "value": "yes", "text":"$(knowDateY)", "next": "askGoalDate"},
      { "name": "knowDateN", "value": "no", "text":"$(knowDateN)", "next": "askGoalWeeklySaveAmount"}
    ]
  },
  {
    "name": "askGoalDate",
    "type": "date",
    "text": "$(askGoalDate)",
    "error": "validationFailDate",
    "answers": [ { "name": "goalDate", "next": "verifyGoalAmount" } ]
  },
  {
    "name": "askGoalWeeklySaveAmount",
    "type": "choice",
    "text": "$(askGoalWeeklySaveAmount)",
    "answers": [
      { "name": "five", "value":  "500", "text":"$(five)", "next": "askGoalPriorSave"},
      { "name": "thousnad", "value": "1000", "text":"$(thousand)", "next": "askGoalPriorSave"},
      { "name": "thousandFive", "value": "1500", "text":"$(thousandFive)", "next": "askGoalPriorSave"},
      { "name": "twoThousand", "value": "2000", "text":"$(twoThousand)", "next": "askGoalPriorSave"},
      { "name": "otherSaveAmount", "value": "other", "text": "$(otherSaveAmount)", "next": "askGoalSaveAmountOther"}
    ]
  },
  {
    "name": "askGoalSaveAmountOther",
    "type": "currency",
    "text": "$(askGoalSaveAmountOther)",
    "error": "validationFailSaveAmountOther",
    "answers": [ { "name": "weeklySaveAmount", "next": "askGoalPriorSave" } ]
  },
  {
    "name": "askGoalPriorSave",
    "type": "choice",
    "text": "$(askGoalPriorSave)",
    "answers": [
      { "name": "hasSavedY", "value": "yes", "text":"$(hasSavedY)", "next": "askPriorSavedAmount"},
      { "name": "hasSavedN", "value": "no", "text":"$(hasSavedN)", "next": "verifyGoalAmount"}
    ]
  },
  {
    "name": "askPriorSavedAmount",
    "type": "currency",
    "text": "$(askPriorSavedAmount)",
    "error": "validationFailPriorSavedAmount",
    "answers": [ { "name": "priorSaveAmount", "next": "infoGoalEnd" } ]
  },
  {
    "name": "verifyGoalAmount",
    "type": "choice",
    "text": "$(verifyGoalAmount)",
    "error": "validationFailPriorSavedAmount",
    "answers": [
      { "name": "goalCheckRT", "value": "reduceWeek", "text":"$(goalCheckRT)", "next": "askKnowGoalDate"},
      { "name": "goalCheckRA", "value": "reduceAmount", "text":"$(goalCheckRA)", "next": "askPriorSavedAmount"},
      { "name": "goalCheckOk", "value": "ok", "text":"$(goalCheckOk)", "next": "infoGoalEnd"}
    ]
  },
  {
    "name": "validationFail",
    "type": "info",
    "text": "$(validationFail)",
    "next": "askGoalAmount"
  },
  {
    "name": "validationFailDate",
    "type": "info",
    "text": "$(validationFailDate)",
    "next": "askGoalDate"
  },
  {
  "name": "validationFailSaveAmountOther",
  "type": "info",
  "text": "$(validationFailSaveAmountOther)",
  "next": "askGoalSaveAmountOther"
  },
  {
  "name": "validationFailPriorSavedAmount",
  "type": "info",
  "text": "$(validationFailPriorSavedAmount)",
  "next": "askPriorSavedAmount"
},
  {
    "name": "infoGoalEnd",
    "type": "end",
    "text": "$(infoGoalEnd)"
  }
]
