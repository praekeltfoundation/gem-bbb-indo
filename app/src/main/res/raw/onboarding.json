[
  {
    "name":"askName",
    "text":"$(askName)",
    "type":"initials",
    "error": "validationFailName",
    "answers":[
      {"name":"name", "next": "askMobileNumber"}
    ]

  },
  {
    "name":"askMobileNumber",
    "text":"$(askMobileNumber) ",
    "type":"mobile",
    "error": "validationFailNumber",
    "answers":[
      {"name":"mobileNumber", "next": "askPasswordOne"}
    ]

  },
  {
    "name":"askPasswordOne",
    "text":"$(askPasswordOne) ",
    "type":"password",
    "error": "validationFailPassword",
    "answers":[
      {"name":"passwordOne", "next": "askPasswordTwo"}
    ]

  },
  {
    "name":"askPasswordTwo",
    "text":"$(askPasswordTwo)",
    "completed":"no",
    "type":"password",
    "error": "validationFailPasswordMatch",
    "answers":[
      {"name":"passwordTwo", "next":"@checkPasswordMatch"}
    ]

  },
  {
    "name": "validationFailName",
    "type": "info",
    "text": "$(validationFailName)",
    "next": "askName"
  },
  {
    "name": "validationFailNumber",
    "type": "info",
    "text": "$(validationFailNumber)",
    "next": "askMobileNumber"
  },
  {
    "name": "validationFailPassword",
    "type": "info",
    "text": "$(validationFailPassword)",
    "next": "askPasswordOne"
  },
  {
    "name": "validationFailPasswordMatch",
    "type": "info",
    "text": "$(validationFailPasswordMatch)",
    "next": "askPasswordOne"
  },
  {
    "name": "infoOnboardingEnd",
    "type": "info",
    "text": "$(infoOnboardingEnd)"
  }



]